<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatisplus.org//DTD Mapper 3.0//EN" "http://mybatisplus.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aircjm.study.mybatisplus.mapper.SysUserMapper">
    <update id="updateOne">
        update sys_user set username = #{username} where id = #{id}
    </update>
    <select id="selectOne" resultType="com.aircjm.study.mybatisplus.domain.SysUser">
        select  * from sys_user where  id = #{id}
    </select>
</mapper>